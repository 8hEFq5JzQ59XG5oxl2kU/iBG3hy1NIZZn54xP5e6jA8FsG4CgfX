                        0,
                        1,
                        -0,
                        0.000283906702,
                        0,
                        0.99999994
                    )
                end
            end

            local player = game.Players.LocalPlayer
            local part = Instance.new("Part")
            part.Anchored = true
            part.BottomSurface = Enum.SurfaceType.Smooth
            part.TopSurface = Enum.SurfaceType.Smooth
            part.Color = Color3.fromRGB(255, 0, 0)
            part.Material = Enum.Material.Neon
            part.Size = Vector3.new(6, 1, 6)
            part.Parent = workspace
            part.Name = "slowpihax-autofarm"

            local partloop =
                coroutine.wrap(
                function()
                    while dopartloop == true do
                        if dopartloop == true then
                            if bool == true then
                                task.wait()
                                part.CFrame =
                                    CFrame.new(player.Character.HumanoidRootPart.Position - Vector3.new(0, 3.348115, 0))
                            elseif bool == false then
                                task.wait()
                                dopartloop = false
                            end
                        end
                    end
                end
            )

            local autofarmloop =
                coroutine.wrap(
                function()
                    while autofarmreturn == true do
                        local character = game.Players.LocalPlayer.Character
                        local humanoid = character:WaitForChild("Humanoid")
                        local startCFrame = character.HumanoidRootPart.CFrame
                        local endCFrame = CFrame.new(Vector3.new(237.068024, 145.547531, -433.277161))
                        local duration = 2
                        local startTime = tick()

                        while tick() - startTime < duration do
                            local t = (tick() - startTime) / duration
                            character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                            task.wait()
                        end

                        game.Players.LocalPlayer.Character.Humanoid.Jump = true

                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-765.508301, 66.498024, 100.494644))
                            local duration = 2
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-996.701904, 77.3839951, 314.132507))
                            local duration = 1
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-1191.66199, 77.3840103, 314.631927))
                            local duration = 1
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-1295.64136, 67.998024, -72.0887909))
                            local duration = 2
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-1168.98181, 67.9980011, -801.711914))
                            local duration = 2
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(-821.609009, 19.9980259, -1416.73657))
                            local duration = 2
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(396.152283, 25.99802303, -1416.4646))
                            local duration = 3
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                        if autofarm == true then
                            local character = game.Players.LocalPlayer.Character
                            local humanoid = character:WaitForChild("Humanoid")
                            local startCFrame = character.HumanoidRootPart.CFrame
                            local endCFrame = CFrame.new(Vector3.new(369.216003, 86.6812973, -1383.78796))
                            local duration = 2
                            local startTime = tick()

                            while tick() - startTime < duration do
                                local t = (tick() - startTime) / duration
                                character.HumanoidRootPart.CFrame = startCFrame:lerp(endCFrame, t)
                                task.wait()
                            end
                            game.Players.LocalPlayer.Character.Humanoid.Jump = true
                        end
                    end
                end
            )

            partloop()
            autofarmloop()
        end
    }
)

