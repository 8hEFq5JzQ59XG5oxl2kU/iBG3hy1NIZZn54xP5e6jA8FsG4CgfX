                        3.15943041e-08,
                        -0.107249357
                    )
                elseif randomteleport == 3 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                        CFrame.new(
                        -58.4512482,
                        17.9980431,
                        -1202.94751,
                        -0.999971867,
                        3.98565057e-08,
                        0.0075365724,
                        4.01600779e-08,
                        1,
                        4.01282492e-08,
                        -0.0075365724,
                        4.04297822e-08,
                        -0.999971867
                    )
                elseif randomteleport == 4 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                        CFrame.new(
                        -452.512634,
                        26.0980225,
                        -1111.76917,
                        0.999878585,
                        -0,
                        -0.0155818816,
                        0,
                        1,
                        -0,
                        0.0155818816,
                        0,
                        0.999878585
                    )
                elseif randomteleport == 5 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                        CFrame.new(
