        Section2:Toggle({
            Text = "Bot ESP",
            Callback = function(bool)
                function doBotEsp_mall()
                    if bool == true then
                        for i,v in pairs(game:GetService("Workspace").bots["nn_mall"]:GetDescendants()) do
                        if v.ClassName == "Humanoid" and v.Parent.Name ~= "slowpihax on top" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                        local BillboardGui = Instance.new("BillboardGui")
                        local TextLabel = Instance.new("TextLabel")
                        local botName = v.Parent.Name
                        
                        BillboardGui.Name = "BotEsp_slowpihax"
                        BillboardGui.Parent = v.Parent
                        BillboardGui.AlwaysOnTop = true
                        BillboardGui.LightInfluence = 1
                        BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                        BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
                        
                        TextLabel.Parent = BillboardGui
                        TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                        TextLabel.BackgroundTransparency = 1
                        TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                        TextLabel.Text = botName
                        TextLabel.TextColor3 = Color3.new(1, 0, 0)
                        TextLabel.TextScaled = false
                                end
                            end
                        elseif bool == false then
                    end
                end
                    
