            Text = "Credits"
        })
        
        -- Sections
    
        local Section = Tab:Section({
            Text = "Character"
        })

        local Section2 = Tab2:Section({
            Text = "Visual"
        })
    
        local Section3 = Tab3:Section({
            Text = "Autofarm"
        })
    
