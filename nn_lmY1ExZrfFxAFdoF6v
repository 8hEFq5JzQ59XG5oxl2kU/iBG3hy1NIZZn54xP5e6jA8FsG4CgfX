        -- Section 3

        Section3:Toggle({
            Text = "Autofarm",
            Callback = function(bool)
                if bool == true then

                    local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                    local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                        Notification:Notify(
                            {Title = "Autofarm Enabled", Description = "Autofarm Is Now Enabled."},
                            {OutlineColor = Color3.fromRGB(80, 80, 80),Time = 2, Type = "default"}
                        )

                autofarm = true
                autofarmreturn = true

        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-121.19397, 11.7480297, -425.320862, -0.0171290636, -1.7715598e-05, 0.999853373, -1.74467186e-05, 1, 1.74193101e-05, -0.999853373, -1.7145785e-05, -0.0171290636)
            
local TS = game:GetService("TweenService")
while autofarmreturn == true do

    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(2), {CFrame = CFrame.new(237.068024, 145.547531, -433.277161)})
        tween:Play()
            tween.Completed:wait()
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    task.wait(0.4)

if autofarm == true then
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(2), {CFrame = CFrame.new(-765.508301, 66.498024, 100.494644)})
        tween:Play()
            tween.Completed:wait()
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    task.wait(0.4)

end
if autofarm == true then
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(2), {CFrame = CFrame.new(-996.701904, 77.3839951, 314.132507)})
        tween:Play()
            tween.Completed:wait()
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    task.wait(0.4)

end
if autofarm == true then
    local tween = TS:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(2), {CFrame = CFrame.new(-1191.66199, 77.3840103, 314.631927)})
        tween:Play()
            tween.Completed:wait()
        game.Players.LocalPlayer.Character.Humanoid.Jump = true
    task.wait(0.4)

