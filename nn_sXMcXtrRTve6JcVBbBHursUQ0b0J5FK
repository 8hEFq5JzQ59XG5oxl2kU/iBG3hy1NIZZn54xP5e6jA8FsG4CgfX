getgenv().KeybindFly = 0
getgenv().KeybindInfJumps = 0
getgenv().KeybindGodmode = 0
getgenv().KeybindBoxEsp = 0
getgenv().KeybindTracerEsp = 0
getgenv().KeybindChamsEsp = 0
