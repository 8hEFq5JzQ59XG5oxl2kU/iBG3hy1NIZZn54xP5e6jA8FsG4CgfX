                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().espLib =
                    loadstring(
                    game:HttpGet(
                        ("https://raw.githubusercontent.com/shlexware/Sirius/request/library/esp/esp.lua"),
                        true
                    )
                )()

                getgenv().espLib.options.enabled = true
                getgenv().espLib.options.minScaleFactorX = 1
                getgenv().espLib.options.maxScaleFactorX = 10
                getgenv().espLib.options.minScaleFactorY = 1
                getgenv().espLib.options.maxScaleFactorY = 10
                getgenv().espLib.options.boundingBox = false -- WARNING | Significant Performance Decrease when true
                getgenv().espLib.options.boundingBoxDescending = true
                getgenv().espLib.options.font = 2
                getgenv().espLib.options.fontSize = 13
                getgenv().espLib.options.limitDistance = false
                getgenv().espLib.options.maxDistance = 99999
                getgenv().espLib.options.visibleOnly = false
                getgenv().espLib.options.teamCheck = false
                getgenv().espLib.options.teamColor = true
                getgenv().espLib.options.fillColor = nil
                getgenv().espLib.options.whitelistColor = Color3.new(1, 0, 0)
                getgenv().espLib.options.outOfViewArrows = false
                getgenv().espLib.options.outOfViewArrowsFilled = false
                getgenv().espLib.options.outOfViewArrowsSize = 25
                getgenv().espLib.options.outOfViewArrowsRadius = 100
                getgenv().espLib.options.outOfViewArrowsColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.outOfViewArrowsTransparency = 0.5
                getgenv().espLib.options.outOfViewArrowsOutline = false
                getgenv().espLib.options.outOfViewArrowsOutlineFilled = false
                getgenv().espLib.options.outOfViewArrowsOutlineColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.outOfViewArrowsOutlineTransparency = 1
                getgenv().espLib.options.names = false
                getgenv().espLib.options.nameTransparency = 1
                getgenv().espLib.options.nameColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.boxes = false
                getgenv().espLib.options.boxesTransparency = 1
                getgenv().espLib.options.boxesColor = Color3.new(1, 0, 0)
                getgenv().espLib.options.boxFill = false
                getgenv().espLib.options.boxFillTransparency = 0.5
                getgenv().espLib.options.boxFillColor = Color3.new(1, 0, 0)
                getgenv().espLib.options.healthBars = false
                getgenv().espLib.options.healthBarsSize = 1
                getgenv().espLib.options.healthBarsTransparency = 1
                getgenv().espLib.options.healthBarsColor = Color3.new(0, 1, 0)
                getgenv().espLib.options.healthText = false
                getgenv().espLib.options.healthTextTransparency = 1
                getgenv().espLib.options.healthTextSuffix = "%"
                getgenv().espLib.options.healthTextColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.distance = false
                getgenv().espLib.options.distanceTransparency = 1
                getgenv().espLib.options.distanceSuffix = " Studs"
                getgenv().espLib.options.distanceColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.tracers = false
                getgenv().espLib.options.tracerTransparency = 1
                getgenv().espLib.options.tracerColor = Color3.new(1, 1, 1)
                getgenv().espLib.options.tracerOrigin = "Bottom" -- Available [Mouse, Top, Bottom]
                getgenv().espLib.options.chams = true
                getgenv().espLib.options.chamsFillColor = Color3.new(0, 255, 0)
                getgenv().espLib.options.chamsFillTransparency = 0.45
                getgenv().espLib.options.chamsOutlineColor = Color3.new()
                getgenv().espLib.options.chamsOutlineTransparency = 0.05

                getgenv().espLib:Load()
            else
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Chams Disabled", Description = "You Can No Longer See Players Through Walls."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().espLib:Unload()
            end
        end
    }
)

getgenv().Section6_1:Keybind(
    {
        Text = "Bot Esp Keybind",
        Default = Enum.KeyCode.Clear,
        Callback = function()
            if getgenv().KeybindBotEsp == 0 then
                getgenv().KeybindBotEsp = getgenv().KeybindBotEsp + 1
            elseif getgenv().KeybindBotEsp == 1 then
                getgenv().KeybindBotEsp = getgenv().KeybindBotEsp - 1
            end
            if getgenv().KeybindBotEsp == 1 then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Bot Esp Enabled", Description = "You Can Now See All The Bots."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                function doBotEsp()
                    if getgenv().KeybindBotEsp == 1 then
                        getgenv().BotESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

                        getgenv().BotESP.Tracers = true
                        getgenv().BotESP.Names = true
                        getgenv().BotESP.Boxes = true
                        getgenv().BotESP.Players = false

                        getgenv().BotESP:AddObjectListener(
                            workspace.bots,
                            {
                                Recursive = true,
                                Type = "Model",
                                CustomName = function(obj)
                                    return obj:FindFirstChild("bot") and obj.Name
                                end,
                                Color = Color3.fromRGB(255, 0, 0),
                                IsEnabled = "botesp"
                            }
                        )

                        workspace.bots.DescendantRemoving:Connect(
                            function(Descendant)
                                if Descendant:IsA("Model") then
                                    getgenv().BotESP:AddObjectListener(
                                        workspace.bots,
                                        {
                                            Recursive = true,
                                            Type = "Model",
                                            CustomName = function(obj)
                                                return obj:FindFirstChild("bot") and obj.Name
                                            end,
                                            Color = Color3.fromRGB(255, 0, 0),
                                            IsEnabled = "botesp"
                                        }
                                    )
                                end
                            end
                        )

                        workspace.bots.DescendantAdded:Connect(
                            function(Descendant)
                                if Descendant:IsA("Model") then
                                    getgenv().BotESP:AddObjectListener(
                                        workspace.bots,
                                        {
                                            Recursive = true,
                                            Type = "Model",
                                            CustomName = function(obj)
                                                return obj:FindFirstChild("bot") and obj.Name
                                            end,
                                            Color = Color3.fromRGB(255, 0, 0),
                                            IsEnabled = "botesp"
                                        }
                                    )
                                end
                            end
                        )
                    end
                end

                doBotEsp()

                getgenv().BotESP.botesp = true
                getgenv().BotESP:Toggle(true)
            else
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Bot Esp Disabled", Description = "You Can No Longer See All The Bots."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().BotESP.botesp = false
                getgenv().BotESP:Toggle(false)
            end
        end
    }
)

getgenv().Section6_1:Keybind(
    {
        Text = "Powerbox Esp Keybind",
        Default = Enum.KeyCode.Clear,
        Callback = function()
            if getgenv().KeybindboxEsp == 0 then
                getgenv().KeybindboxEsp = getgenv().KeybindboxEsp + 1
            elseif getgenv().KeybindboxEsp == 1 then
                getgenv().KeybindboxEsp = getgenv().KeybindboxEsp - 1
            end
            if getgenv().KeybindboxEsp == 1 then
                for i, v in pairs(game:GetService("Workspace")["nn_russia"][".powerboxes"]:GetDescendants()) do
                    if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                        local BillboardGui = Instance.new("BillboardGui")
                        local TextLabel = Instance.new("TextLabel")
                        local botName = v.Parent.Name

                        BillboardGui.Name = "BoxEsp_slowpihax"
                        BillboardGui.Parent = v.Parent
                        BillboardGui.AlwaysOnTop = true
                        BillboardGui.LightInfluence = 1
                        BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                        BillboardGui.StudsOffset = Vector3.new(0, 2, 0)

                        TextLabel.Parent = BillboardGui
                        TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                        TextLabel.BackgroundTransparency = 1
                        TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                        TextLabel.Text = botName
                        TextLabel.TextColor3 = Color3.new(0, 255, 0)
                        TextLabel.TextScaled = false
                    end
                end
            else
            end

            function doPowerBoxEsp_mall()
                if getgenv().KeybindboxEsp == 1 then
                    for i, v in pairs(game:GetService("Workspace")["nn_mall"][".powerboxes"]:GetDescendants()) do
                        if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                            local BillboardGui = Instance.new("BillboardGui")
                            local TextLabel = Instance.new("TextLabel")

                            BillboardGui.Name = "BoxEsp_slowpihax"
                            BillboardGui.Parent = v.Parent
                            BillboardGui.AlwaysOnTop = true
                            BillboardGui.LightInfluence = 1
                            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                            BillboardGui.StudsOffset = Vector3.new(0, 1, 0)

                            TextLabel.Parent = BillboardGui
                            TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                            TextLabel.BackgroundTransparency = 1
                            TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                            TextLabel.Text = "Power Box"
                            TextLabel.TextColor3 = Color3.new(0, 255, 0)
                            TextLabel.TextScaled = false
                        end
                    end
                else
                end
            end

            function doPowerBoxEsp_russia()
                if getgenv().KeybindboxEsp == 1 then
                    for i, v in pairs(game:GetService("Workspace")["nn_russia"][".powerboxes"]:GetDescendants()) do
                        if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                            local BillboardGui = Instance.new("BillboardGui")
                            local TextLabel = Instance.new("TextLabel")

                            BillboardGui.Name = "BoxEsp_slowpihax"
                            BillboardGui.Parent = v.Parent
                            BillboardGui.AlwaysOnTop = true
                            BillboardGui.LightInfluence = 1
                            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                            BillboardGui.StudsOffset = Vector3.new(0, 1, 0)

                            TextLabel.Parent = BillboardGui
                            TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                            TextLabel.BackgroundTransparency = 1
                            TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                            TextLabel.Text = "Power Box"
                            TextLabel.TextColor3 = Color3.new(0, 255, 0)
                            TextLabel.TextScaled = false
                        end
                    end
                else
                end
            end

            if getgenv().KeybindboxEsp == 1 then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Power Box Enabled", Description = "You Can Now See All The Boxes."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                doPowerBoxEsp_mall()
                task.wait(0.25)
                doPowerBoxEsp_mall()

                if game:GetService("Workspace"):FindFirstChild("PowerInteract") == "nil" then
                    for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                        if v.ClassName == "BillboardGui" and v.Name == "BoxEsp_slowpihax" then
                            v:Destroy()
                        end
                    end
                end
            else
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Power Box Disabled", Description = "You Can No Longer See All The Boxes."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                    if v.ClassName == "BillboardGui" and v.Name == "BoxEsp_slowpihax" then
                        v:Destroy()
                    end
                end
            end
        end
    }
)

getgenv().Section6_2:Keybind(
    {
        Text = "Get Btools Keybind",
        Default = Enum.KeyCode.Clear,
        Callback = function()
            if getgenv().KeybindGetBtools == 0 then
                getgenv().KeybindGetBtools = getgenv().KeybindGetBtools + 1
            elseif getgenv().KeybindGetBtools == 1 then
                getgenv().KeybindGetBtools = getgenv().KeybindGetBtools - 1
            end
            if getgenv().KeybindGetBtools == 1 then
                loadstring(game:HttpGet("https://pastebin.com/raw/PSeXEvEc"))()

                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Btools Enabled", Description = "You Now Have Client Sided Btools."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )
            else
                game:GetService("Players").LocalPlayer.Backpack.Move:Destroy()
                game:GetService("Players").LocalPlayer.Backpack.Delete:Destroy()
                game:GetService("Players").LocalPlayer.Backpack.Undo:Destroy()
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Btools Disabled", Description = "You No Longer Have Client Sided Btools."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )
            end
        end
    }
)

getgenv().Section6_2:Keybind(
    {
        Text = "FPS Boost Keybind",
        Default = Enum.KeyCode.Clear,
        Callback = function()
            if getgenv().KeybindFPSBoost == 0 then
                getgenv().KeybindFPSBoost = getgenv().KeybindFPSBoost + 1
            elseif getgenv().KeybindFPSBoost == 1 then
                getgenv().KeybindFPSBoost = getgenv().KeybindFPSBoost - 1
            end
            if getgenv().KeybindFPSBoost == 1 then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "FPS Boost Enabled", Description = "Your FPS Should Now Be Higher."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                local a = game
                local b = a.Workspace
                local c = a.Lighting
                local d = b.Terrain
                d.WaterWaveSize = 0
                d.WaterWaveSpeed = 0
                d.WaterReflectance = 0
                d.WaterTransparency = 0
                c.GlobalShadows = false
                c.FogEnd = 9e9
                settings().Rendering.QualityLevel = "Level01"

                while task.wait() do
                    if iswindowactive() then
                        setfpscap(144)
                    else
                        setfpscap(25)
                    end
                end
            elseif bool == false then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Noclip", Description = "Your FPS Should Now Be Back To Normal."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2.25, Type = "default"}
                )

                local a = game
                local b = a.Workspace
                local c = a.Lighting
                local d = b.Terrain
                d.WaterWaveSize = 0.15
                d.WaterWaveSpeed = 10
                d.WaterReflectance = 1
                d.WaterTransparency = 1
                c.GlobalShadows = true
                c.FogEnd = 100000
                settings().Rendering.QualityLevel = "Level20"
            end

            while task.wait() do
                if iswindowactive() then
                    setfpscap(144)
                else
                    setfpscap(60)
                end
            end
        end
    }
)

loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/0KUNh1t5jCWeTAFM/8mqsGtYULMu9MfI8/main/nn_b-akkeybind"))()

if game.PlaceId ~= 10118559731 then
    local drop =
        getgenv().Section7:Dropdown(
        {
            Text = "Nico's Nextbots | Working",
            List = {"Teleport"},
            Callback = function(v)
                local placeid = 10118559731 -- Enter your place ID

                for i, v in pairs(game:FindFirstChildOfClass("Players"):GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        game:GetService("TeleportService"):Teleport(placeid, v)
                    end
                end
                task.wait(0.5)
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Teleport Failed", Description = "Please Join The Game Manually."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 5, Type = "default"}
                )
            end
        }
    )
end

