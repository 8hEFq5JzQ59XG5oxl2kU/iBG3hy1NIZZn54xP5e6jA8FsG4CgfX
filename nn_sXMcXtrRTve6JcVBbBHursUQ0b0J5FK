                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().espLib:Unload()
            end
        end
    }
)

getgenv().Section2:Toggle(
    {
        Text = "Bot Esp",
        Callback = function(bool)
            if bool == true then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Bot Esp Enabled", Description = "You Can Now See All The Bots."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                function doBotEsp()
                    if bool == true then
                        getgenv().BotESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()

                        getgenv().BotESP.Tracers = true
                        getgenv().BotESP.Names = true
                        getgenv().BotESP.Boxes = true
                        getgenv().BotESP.Players = false

                        getgenv().BotESP:AddObjectListener(
                            workspace.bots,
                            {
                                Recursive = true,
                                Type = "Model",
                                CustomName = function(obj)
                                    return obj:FindFirstChild("bot") and obj.Name
                                end,
                                Color = Color3.fromRGB(255, 0, 0),
                                IsEnabled = "botesp"
                            }
                        )

                        workspace.bots.DescendantRemoving:Connect(
                            function(Descendant)
                                if Descendant:IsA("Model") then
                                    getgenv().BotESP:AddObjectListener(
                                        workspace.bots,
                                        {
                                            Recursive = true,
                                            Type = "Model",
                                            CustomName = function(obj)
                                                return obj:FindFirstChild("bot") and obj.Name
                                            end,
                                            Color = Color3.fromRGB(255, 0, 0),
                                            IsEnabled = "botesp"
                                        }
                                    )
                                end
                            end
                        )

                        workspace.bots.DescendantAdded:Connect(
                            function(Descendant)
                                if Descendant:IsA("Model") then
                                    getgenv().BotESP:AddObjectListener(
                                        workspace.bots,
                                        {
                                            Recursive = true,
                                            Type = "Model",
                                            CustomName = function(obj)
                                                return obj:FindFirstChild("bot") and obj.Name
                                            end,
                                            Color = Color3.fromRGB(255, 0, 0),
                                            IsEnabled = "botesp"
                                        }
                                    )
                                end
                            end
                        )
                    end
                end

                doBotEsp()

                getgenv().BotESP.botesp = true
                getgenv().BotESP:Toggle(true)
            elseif bool == false then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Bot Esp Disabled", Description = "You Can No Longer See All The Bots."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().BotESP.botesp = false
                getgenv().BotESP:Toggle(false)
            end
        end
    }
)

Section2:Toggle(
    {
        Text = "Power Box ESP",
        Callback = function(bool)
            function doPowerBoxEsp_russia()
                if bool == true then
                    for i, v in pairs(game:GetService("Workspace")["nn_russia"][".powerboxes"]:GetDescendants()) do
                        if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                            local BillboardGui = Instance.new("BillboardGui")
                            local TextLabel = Instance.new("TextLabel")
                            local botName = v.Parent.Name

                            BillboardGui.Name = "BoxEsp_slowpihax"
                            BillboardGui.Parent = v.Parent
                            BillboardGui.AlwaysOnTop = true
                            BillboardGui.LightInfluence = 1
                            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                            BillboardGui.StudsOffset = Vector3.new(0, 2, 0)

                            TextLabel.Parent = BillboardGui
                            TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                            TextLabel.BackgroundTransparency = 1
                            TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                            TextLabel.Text = botName
                            TextLabel.TextColor3 = Color3.new(0, 255, 0)
                            TextLabel.TextScaled = false
                        end
                    end
                elseif bool == false then
                end
            end

            function doPowerBoxEsp_mall()
                if bool == true then
                    for i, v in pairs(game:GetService("Workspace")["nn_mall"][".powerboxes"]:GetDescendants()) do
                        if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                            local BillboardGui = Instance.new("BillboardGui")
                            local TextLabel = Instance.new("TextLabel")

                            BillboardGui.Name = "BoxEsp_slowpihax"
                            BillboardGui.Parent = v.Parent
                            BillboardGui.AlwaysOnTop = true
                            BillboardGui.LightInfluence = 1
                            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                            BillboardGui.StudsOffset = Vector3.new(0, 1, 0)

                            TextLabel.Parent = BillboardGui
                            TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                            TextLabel.BackgroundTransparency = 1
                            TextLabel.Size = UDim2.new(1.25, 0, 1.25, 0)
                            TextLabel.Text = "Power Box"
                            TextLabel.TextColor3 = Color3.new(0, 255, 0)
                            TextLabel.TextScaled = false
                        end
                    end
                elseif bool == false then
                end
            end

            function doPowerBoxEsp_russia()
                if bool == true then
                    for i, v in pairs(game:GetService("Workspace")["nn_russia"][".powerboxes"]:GetDescendants()) do
                        if v.Name == "PowerInteract" and v.Parent.Name == "PowerBox" then -- i am just wondering that the part u wanna esp have these properties u can change them if it doesnt work
                            local BillboardGui = Instance.new("BillboardGui")
                            local TextLabel = Instance.new("TextLabel")

                            BillboardGui.Name = "BoxEsp_slowpihax"
                            BillboardGui.Parent = v.Parent
                            BillboardGui.AlwaysOnTop = true
                            BillboardGui.LightInfluence = 1
                            BillboardGui.Size = UDim2.new(0, 50, 0, 50)
