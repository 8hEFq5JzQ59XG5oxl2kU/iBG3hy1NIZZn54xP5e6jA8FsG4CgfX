getgenv().Section6_2:Keybind(
    {
        Text = "FPS Boost Keybind",
        Default = Enum.KeyCode.Clear,
        Callback = function()
            if getgenv().KeybindFPSBoost == 0 then
                getgenv().KeybindFPSBoost = getgenv().KeybindFPSBoost + 1
            elseif getgenv().KeybindFPSBoost == 1 then
                getgenv().KeybindFPSBoost = getgenv().KeybindFPSBoost - 1
            end
            if getgenv().KeybindFPSBoost == 1 then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "FPS Boost Enabled", Description = "Your FPS Should Now Be Higher."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                local a = game
                local b = a.Workspace
                local c = a.Lighting
                local d = b.Terrain
                d.WaterWaveSize = 0
                d.WaterWaveSpeed = 0
                d.WaterReflectance = 0
                d.WaterTransparency = 0
                c.GlobalShadows = false
                c.FogEnd = 9e9
                settings().Rendering.QualityLevel = "Level01"

                while task.wait() do
                    if iswindowactive() then
                        setfpscap(144)
                    else
                        setfpscap(25)
                    end
                end
            elseif bool == false then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Noclip", Description = "Your FPS Should Now Be Back To Normal."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2.25, Type = "default"}
                )

                local a = game
                local b = a.Workspace
                local c = a.Lighting
                local d = b.Terrain
                d.WaterWaveSize = 0.15
                d.WaterWaveSpeed = 10
                d.WaterReflectance = 1
                d.WaterTransparency = 1
                c.GlobalShadows = true
                c.FogEnd = 100000
                settings().Rendering.QualityLevel = "Level20"
            end

            while task.wait() do
                if iswindowactive() then
                    setfpscap(144)
                else
                    setfpscap(60)
                end
            end
        end
    }
)

if getgenv().xw0DLBPTqZkzEWOXx6qCScoNKSoBhGh or getgenv().UtlpHuOwhroliTt4DRSwzv4qdQYuK9M == true then
    getgenv().Section6_2:Keybind(
        {
            Text = "Auto Kill Keybind",
            Default = Enum.KeyCode.Clear,
            Callback = function()
                if getgenv().KeybindAutoKill == 0 then
                    getgenv().KeybindAutoKill = getgenv().KeybindAutoKill + 1
                elseif getgenv().KeybindAutoKill == 1 then
                    getgenv().KeybindAutoKill = getgenv().KeybindAutoKill - 1
                end
                local AutoKillingCo = coroutine.create(function()
                    while getgenv().KeybindAutoKill == 1 do
                    task.wait()
                    local radius = 3
                    local yOffset = math.random(-75, 130)/100
                    local angle = math.random(0, 2*math.pi)
                    local offset = Vector3.new(math.cos(angle)*radius, yOffset, math.sin(angle)*radius)
                    local Directionmulti = Vector3.new(math.random(80, 160)/100, math.random(20, 175)/100, math.random(80, 160)/100)
                    local players = game:GetService("Players"):GetPlayers()
                    local notFoundPlayers = {}

                    local teleportStartTime = tick()
                    local teleportCurrentTime = tick()

                    if teleportCurrentTime - teleportStartTime >= 5 then
                        for i = 1, 3 do
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-107.0163345336914, 85.84809875488281, -805.146484375)
                                task.wait(0.25)
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1110.2471923828125, 89.2927017211914, -92.7923355102539)
                        end
                        teleportStartTime = teleportCurrentTime
                    else
                        task.wait()
                    end
                    

                    for _, player in pairs(players) do
                        if player.Character then
                            local humanoid = player.Character:FindFirstChild("HumanoidRootPart")
                            if humanoid then
                                if player.Character.scripts.values.safe.Value == false then
                                    if player.Character.scripts.values.botpossessed.Value == "" then
                                        table.insert(notFoundPlayers, player)
                                    end
                                end
                            end
                        end
                    end

                    local players = notFoundPlayers
                    local closestPlayer = nil
                    local closestDistance = math.huge

                    closestPlayer = nil
                    closestDistance = math.huge
                    for _, player in pairs(players) do
                        if
                            player.Character and player.Character:FindFirstChild("HumanoidRootPart") and
                                player ~= game.Players.LocalPlayer and
                                player.Character.Humanoid.Health ~= 0
                        then
                            local distance =
                                (player.Character.HumanoidRootPart.Position -
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude
                            if distance < closestDistance then
                                closestPlayer = player
                                closestDistance = distance
                            end
                        end
                    end
                    if closestPlayer and _G.DoTeleports then
                        local distance = (closestPlayer.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude
                        if distance <= getgenv().TPAuraRange then
                            local x = closestPlayer.Character.Head.CFrame.lookVector.X
                            local z = closestPlayer.Character.Head.CFrame.lookVector.Z
                            local newVector = Vector3.new(x, 0, z)

                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(closestPlayer.Character.HumanoidRootPart.Position + offset) + (newVector * math.random(135, 250)/100)
                        end
                    end
                    notFoundPlayers = {}
                    gcinfo("count")
                    gcinfo("collect")
                end
            end)
                
            if getgenv().KeybindAutoKill == 1 then
                _G.DoTeleports = true
                coroutine.resume(AutoKillingCo)
            else
                _G.DoTeleports = false
                coroutine.yield(AutoKillingCo)
                notFoundPlayers = {}
                lastPos = currentPos
                gcinfo("count")
                gcinfo("collect")
            end
        end
    })
end

