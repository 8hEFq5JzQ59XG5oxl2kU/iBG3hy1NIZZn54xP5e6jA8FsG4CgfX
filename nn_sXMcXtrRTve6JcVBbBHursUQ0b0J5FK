                    )
                elseif randomteleport == 9 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                        CFrame.new(
                        -796.628784,
                        0.40174341,
                        -68.4246826,
                        -0.0293315649,
                        0,
                        0.999569833,
                        0,
                        1,
