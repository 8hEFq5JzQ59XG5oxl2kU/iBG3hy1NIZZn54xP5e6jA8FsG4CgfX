loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/0KUNh1t5jCWeTAFM/8mqsGtYULMu9MfI8/main/u_title"))()

-- Values

getgenv().FlySpeed = 50

local sped = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
local jmp = game.Players.LocalPlayer.Character.Humanoid.JumpPower

local plr = game:GetService("Players").LocalPlayer
local Spoof
Spoof =
    hookmetamethod(
    game,
    "__index",
    function(self, ...)
        if ... == "WalkSpeed" then
            return sped
        end
        if ... == "JumpPower" then
            return jmp
        end
        return Spoof(self, ...)
    end
)

