-- Sliders

getgenv().DoWalkSpeed = false
getgenv().DoJumpPower = false
getgenv().DoGravity = false

local speedstring = tostring(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed):sub(1, 4)
local walkspeed = tonumber(speedstring)

getgenv().Section:Slider(
    {
        Text = "Walkspeed",
        Default = walkspeed,
        Minimum = 1,
        Maximum = 200,
        Callback = function(Value)
            spawn(function()
                while getgenv().DoWalkSpeed == true do
                    task.wait()
                    if getgenv().DoWalkSpeed == true then
                        local currentVelocity = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
                        local newVelocity = Vector3.new(game.Players.LocalPlayer.Character.Humanoid.MoveDirection.X * Value, currentVelocity.Y, game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Z * Value)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = newVelocity
                    end
                end
            end)
        end
    }
)

getgenv().Section:Toggle(
    {
        Text = "Walkspeed Toggle",
        Callback = function(bool)
            if bool == true then
                getgenv().DoWalkSpeed = true
            else
                getgenv().DoWalkSpeed = false
            end
        end
    }
)

local jumpstring = tostring(game.Players.LocalPlayer.Character.Humanoid.JumpPower):sub(1, 4)
local jumppower = tonumber(jumpstring)

getgenv().thejumppower = jumppower

getgenv().newwstate = Enum.HumanoidStateType.Landed

getgenv().MakeJump = true

local player = game.Players.LocalPlayer

player.Character.Humanoid.StateChanged:Connect(function(oldState, newState)
    getgenv().newwstate = newState

    if newState == Enum.HumanoidStateType.Freefall then
        getgenv().MakeJump = false
    else
        getgenv().MakeJump = true
    end
end)


local userInputService = game:GetService("UserInputService")

game:GetService("UserInputService").InputBegan:Connect(function(inputObject)
    if inputObject.KeyCode == Enum.KeyCode.Space then
        if getgenv().DoJumpPower and getgenv().MakeJump == true then
            task.wait(0.01)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.X, getgenv().thejumppower, game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Z)   
        end
    end
end)

getgenv().Section:Slider(
    {
        Text = "JumpPower",
        Default = jumpstring,
        Minimum = 1,
        Maximum = 500,
        Callback = function(Value)
            getgenv().thejumppower = Value
        end
    }
)

getgenv().Section:Toggle(
    {
        Text = "JumpPower Toggle",
        Callback = function(bool)
            if bool == true then
                getgenv().DoJumpPower = true
            else
                getgenv().DoJumpPower = false
                getgenv().thejumppower = jumppower
            end
        end
    }
)

local gravitystring = tostring(game.Workspace.Gravity):sub(1, 4)
local gravity = tonumber(gravitystring)
local oldgravity = tonumber(game.Workspace.Gravity)

getgenv().Section:Slider(
    {
        Text = "Gravity",
        Default = gravity,
        Minimum = 0,
        Maximum = 540,
        Callback = function(Value)
            if getgenv().DoGravity == true then
                game.Workspace.Gravity = Value
            end
        end
    }
)

getgenv().Section:Toggle(
    {
        Text = "Gravity Toggle",
        Callback = function(bool)
            if bool == true then
                getgenv().DoGravity = true
            else
                getgenv().DoGravity = false
                game.Workspace.Gravity = oldgravity
            end
        end
    }
)

getgenv().Section:Slider(
    {
        Text = "FlySpeed",
        Default = (getgenv().FlySpeed),
        Minimum = 1,
        Maximum = 350,
        Callback = function(Value)
            getgenv().FlySpeed = (Value)
        end
    }
)

--Section 1

getgenv().Section:Toggle(
    {
        Text = "Fly (R15)",
        Callback = function(bool)
            _G.FLYING = false

            if bool == true then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Fly Enabled", Description = "You Are Now Flying."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                _G.FLYING = true
                local LP = game:service("Players").LocalPlayer
                local T = LP.Character.UpperTorso
                local CONTROL = {F = 0, B = 0, L = 0, R = 0}
                local lCONTROL = {F = 0, B = 0, L = 0, R = 0}
                local SPEED = getgenv().FlySpeed
                local MOUSE = LP:GetMouse()

                local function FLY()
                    _G.FLYING = true
                    local BG = Instance.new("BodyGyro", T)
                    local BV = Instance.new("BodyVelocity", T)
                    BG.P = 9e4
                    BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                    BG.cframe = T.CFrame
                    BV.velocity = Vector3.new(0, 0.1, 0)
                    BV.maxForce = Vector3.new(9e9, 9e9, 9e9)

                    spawn(
                        function()
                            repeat
                                wait()
                                LP.Character.Humanoid.PlatformStand = true
                                if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 then
                                    SPEED = getgenv().FlySpeed
                                elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0) and SPEED ~= 0 then
                                    SPEED = 0
                                end
                                if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 then
                                    BV.velocity =
                                        ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector *
                                        (CONTROL.F + CONTROL.B)) +
                                        ((game.Workspace.CurrentCamera.CoordinateFrame *
                                            CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B) * 0.2, 0).p) -
                                            game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                        SPEED
                                    lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
                                elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and SPEED ~= 0 then
                                    BV.velocity =
                                        ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector *
                                        (lCONTROL.F + lCONTROL.B)) +
                                        ((game.Workspace.CurrentCamera.CoordinateFrame *
                                            CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B) * 0.2, 0).p) -
                                            game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                        SPEED
                                else
                                    BV.velocity = Vector3.new(0, 0.1, 0)
                                end
                                BG.cframe = game.Workspace.CurrentCamera.CoordinateFrame
                            until not _G.FLYING
                            CONTROL = {F = 0, B = 0, L = 0, R = 0}
                            lCONTROL = {F = 0, B = 0, L = 0, R = 0}
                            SPEED = 0
                            BG:destroy()
                            BV:destroy()
                            LP.Character.Humanoid.PlatformStand = false
                        end
                    )
                end

                MOUSE.KeyDown:connect(
                    function(KEY)
                        if KEY:lower() == "w" then
                            CONTROL.F = 1
                        elseif KEY:lower() == "s" then
                            CONTROL.B = -1
                        elseif KEY:lower() == "a" then
                            CONTROL.L = -1
                        elseif KEY:lower() == "d" then
                            CONTROL.R = 1
                        end
                    end
                )

                MOUSE.KeyUp:connect(
                    function(KEY)
                        if KEY:lower() == "w" then
                            CONTROL.F = 0
                        elseif KEY:lower() == "s" then
                            CONTROL.B = 0
                        elseif KEY:lower() == "a" then
                            CONTROL.L = 0
                        elseif KEY:lower() == "d" then
                            CONTROL.R = 0
                        end
                    end
                )

                FLY()
            else
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Fly Disabled", Description = "You Are No Longer Flying."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                _G.FLYING = false
            end
        end
    }
)

getgenv().Section:Toggle(
    {
        Text = "Inf Jumps",
        Callback = function(bool)
            if bool == true then
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Inf Jumps Enabled", Description = "You Now Have Infinite Jumps."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().InfiniteJumpEnabled = true
            else
                local NotificationHolder =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
                local Notification =
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
                Notification:Notify(
                    {Title = "Inf Jumps Disabled", Description = "You No Longer Have Infinite Jumps."},
                    {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 2, Type = "default"}
                )

                getgenv().InfiniteJumpEnabled = false
            end

            local userInputService = game:GetService("UserInputService")

            game:GetService("UserInputService").InputBegan:Connect(function(inputObject)
                if inputObject.KeyCode == Enum.KeyCode.Space then
                    if getgenv().InfiniteJumpEnabled == true then
                        task.wait(0.01)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.X, getgenv().thejumppower, game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Z)   
                    end
                end
            end)
        end
    }
)

loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/0KUNh1t5jCWeTAFM/8mqsGtYULMu9MfI8/main/nn_godmode"))()

getgenv().Section:Toggle(
    {
        Text = "Invisibility",
        Callback = function(bool)
            if bool == true then
                local player = game.Players.LocalPlayer
                local character = player.Character
                local oldpos = character.HumanoidRootPart.Position
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(321.9046630859375, 9.848121643066406, -1291.5767822265625)
                task.wait(0.25)

                local descendants = character:GetChildren()
                for _, descendant in pairs(descendants) do
                    if descendant:IsA("MeshPart") or descendant:IsA("Part") then
                        descendant.CanCollide = true

                        if descendant.Name == "LowerTorso" then
                            descendant:Destroy()
                        end
                    end
                end

                task.wait(12)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos + Vector3.new(0,2,0))
            else
                game:GetService("ReplicatedStorage").events.player.char.respawnchar:FireServer()
            end
        end
    }
)

getgenv().Section:Button(
    {
        Text = "Force Reset",
        Callback = function()
            game.Players.LocalPlayer.Character.Humanoid.Health = 0
        end
    }
)

